#!/home/yakovlev/gh/yaplans.github.com/.ve_blog/bin/python2
# EASY-INSTALL-ENTRY-SCRIPT: 'Unidecode==1.0.23','console_scripts','unidecode'
__requires__ = 'Unidecode==1.0.23'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Unidecode==1.0.23', 'console_scripts', 'unidecode')()
    )
