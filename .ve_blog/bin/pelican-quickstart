#!/home/yakovlev/gh/yaplans.github.com/.ve_blog/bin/python2
# EASY-INSTALL-ENTRY-SCRIPT: 'pelican==4.0.1','console_scripts','pelican-quickstart'
__requires__ = 'pelican==4.0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pelican==4.0.1', 'console_scripts', 'pelican-quickstart')()
    )
